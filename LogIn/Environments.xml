  <ServicesConfig type="UBS.Core.LaunchPadGUI.ServicesConfig"  UseLocalList="false" ShowTestLinks="false" DownloadTimeOut="30000">

    <!-- Hong Kong R4 -->
    <EnvironmentConfig Name="Hong Kong R4" AppId="sapp_hk">
      <!-- domain in hkg or domain with no environment config will go to HK -->
      <DnsSuffix>hkg.swissbank.com,swissbank.com</DnsSuffix>
      <ServiceLocation ServiceName="Authentication">
        <!-- Accepts multiple urls - will round-robin the services on error -->
        <Url> http://hkg1sapp142.hkg:16060/AuthenticationService</Url>
        <Url> http://hkg2sapp246.hkg:16064/AuthenticationService</Url>
      </ServiceLocation>
      <ServiceLocation ServiceName="SoftwareUpdate">
        <!-- Accepts multiple urls - will round-robin the services on error -->
        <Url> http://hkg1sapp142.hkg:16070/LaunchService</Url>
        <Url> http://hkg2sapp246.hkg:16074/LaunchService</Url>
      </ServiceLocation>
      <ServiceLocation ServiceName="Config">
        <!-- Accepts multiple urls - will round-robin the services on error -->
        <Url> http://hkg1sapp141.hkg:16000/DIRT</Url>
        <Url> http://hkg2sapp242.hkg:16054/DIRT</Url>
      </ServiceLocation>
    </EnvironmentConfig>

